	journal = {Behavioural Brain Research},
	keywords = {Saccade},
	number = {1},
	pages = {41 - 50},
	title = {Adaptation of aimed arm movements to sensorimotor discordance: evidence for direction-independent gain control},
	url = {http://www.sciencedirect.com/science/article/B6SYP-4MD2TYX-6/2/e7b438851f0722076d56fbef3fd69d0b},
	volume = {51},
